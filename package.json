{
  "name": "@indec/form-builder",
  "version": "5.0.0",
  "description": "Form builder",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build:js": "NODE_ENV=production babel src --out-dir lib --delete-dir-on-start --copy-files --no-copy-ignored",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/output.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/output.css --watch",
    "build": "npm run build:js && npm run build:css",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:watch": "jest --config=jest.config.json --watch",
    "prepare": "husky",
    "prettier": "prettier src/ --write",
    "validate": "npm run prettier && npm run lint && npm run test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs-build"
  },
  "keywords": [
    "INDEC",
    "react",
    "node",
    "material",
    "jest",
    "form",
    "builder",
    "questions"
  ],
  "files": [
    "lib",
    "README.md",
    "!lib/**/*.test.js",
    "!lib/**/*.stories.js"
  ],
  "author": "INDEC-it",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@chromatic-com/storybook": "^3.2.2",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.16.0",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/cli": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-webpack5": "^8.4.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.4.2",
    "storybook": "^8.4.7",
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {
    "@storybook/test": "^8.4.7",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.14.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.0.0",
    "react-number-format": "^5.4.2",
    "yup": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git@git.indec.gob.ar:dev/form-builder.git"
  }
}
