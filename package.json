{
  "name": "@indec/form-builder",
  "version": "2.4.3",
  "description": "Form builder",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --delete-dir-on-start --copy-files --no-copy-ignored",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:watch": "jest --config=jest.config.json --watch",
    "prepare": "husky install",
    "validate": "npm run lint && npm run test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs-build"
  },
  "keywords": [
    "INDEC",
    "react",
    "node",
    "material",
    "jest",
    "form",
    "builder",
    "questions"
  ],
  "files": [
    "lib",
    "README.md",
    "!lib/**/*.test.js",
    "!lib/**/*.stories.js"
  ],
  "author": "INDEC-it",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@storybook/addon-actions": "^7.0.26",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/cli": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-webpack5": "^7.0.26",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "storybook": "^7.0.26"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.1",
    "@mui/x-date-pickers": "^6.5.0",
    "date-fns": "^2.30.0",
    "formik": "^2.2.9",
    "framer-motion": "^10.12.12",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-number-format": "^5.3.0",
    "yup": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git@git.indec.gob.ar:dev/form-builder.git"
